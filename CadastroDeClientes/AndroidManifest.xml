<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.mfgs.cadastrodeclientes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_folder_shared_white_48dp"
        android:label="Cadastro de Clientes"
        android:theme="@style/AppTheme" >
        <activity android:name="br.com.mfgs.cadastrodeclientes.ListaClientesActivity">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>	
        </activity>
        
        <activity android:name="br.com.mfgs.cadastrodeclientes.FormularioActivity"/>
        
    </application>

</manifest>
